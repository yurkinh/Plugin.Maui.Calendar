<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit">

    <toolkit:AppThemeColor Light="Black" Dark="Orange" x:Key="DaysLabelTextColor" />
    <toolkit:AppThemeColor Light="Black" Dark="White" x:Key="DeselectedDayLabelTextColor" />
    <toolkit:AppThemeColor Light="Black" Dark="#F7B548" x:Key="DaysTitleLabelTextColor" />
    <toolkit:AppThemeColor Light="Black" Dark="ForestGreen" x:Key="TodayDayLabelTextColor" />
    <toolkit:AppThemeColor Light="Silver" Dark="DimGray" x:Key="OtherMonthDaysLabelTextColor" />
    <toolkit:AppThemeColor Light="Red" Dark="Green" x:Key="EventIndicatorColor" />
    <toolkit:AppThemeColor Light="Red" Dark="Red" x:Key="WeekendDaysLabelTextColor" />
    <toolkit:AppThemeColor Light="White" Dark="#212121" x:Key="BackgroundColor" />


</ResourceDictionary>
