<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit">

    <Color x:Key="EmGreen">#33A78F</Color>
    <Color x:Key="Transparent">Transparent</Color>
    <Color x:Key="RadioButtonColor">#33A78F</Color>
    <toolkit:AppThemeColor
        x:Key="DaysLabelTextColor"
        Dark="Orange"
        Light="Red" />
    <toolkit:AppThemeColor
        x:Key="DeselectedDayLabelTextColor"
        Dark="White"
        Light="Black" />
    <toolkit:AppThemeColor
        x:Key="DaysTitleLabelTextColor"
        Dark="#F7B548"
        Light="Black" />
    <toolkit:AppThemeColor
        x:Key="DaysTitleLabelTextColorMinimalistic"
        Dark="White"
        Light="Black" />

    <toolkit:AppThemeColor
        x:Key="TodayDayLabelTextColor"
        Dark="ForestGreen"
        Light="Black" />
    <toolkit:AppThemeColor
        x:Key="OtherMonthDaysLabelTextColor"
        Dark="DimGray"
        Light="Silver" />
    <toolkit:AppThemeColor
        x:Key="EventIndicatorColor"
        Dark="Green"
        Light="Red" />
    <toolkit:AppThemeColor
        x:Key="WeekendDaysLabelTextColor"
        Dark="Red"
        Light="Red" />
    <toolkit:AppThemeColor
        x:Key="BackgroundColor"
        Dark="#212121"
        Light="White" />
</ResourceDictionary>
